<!DOCTYPE html><html lang="ja"><head><meta charset="utf-8"><title>LaLiga</title></head>
<body>
<h2>ラ・リーガ 一覧</h2>
<p><a href="/">トップへ戻る</a> | <a href="/laliga2/create">新規登録</a></p>

<table border="1">
<tr><th>ID</th><th>チーム名</th><th>都市</th><th>スタジアム</th><th>優勝回数</th></tr>
<% let i=0; for(const row of data){ %>
  <tr>
    <td><%= row.id %></td>
    <td><a href="/laliga2/<%= i %>"><%= row.name %></a></td>
    <td><%= row.city %></td>
    <td><%= row.stadium %></td>
    <td><%= row.titles %></td>
  </tr>
<% i++; } %>
</table>
</body></html>
